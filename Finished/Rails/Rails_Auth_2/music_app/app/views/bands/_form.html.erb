<% if flash[:errors] %>
  <%= render './errors/errors' %>
<% end %>

<%= form_with model: @band, class: 'form' do |f| %>
  <%= auth_token %>
  
  <p>
    <label for="band_name">Name</label>
    <input type="text" name="band[name]" id="band_name" value="<%= @band.name %>">
  </p>

  <input type="submit" value="create">
<% end %>