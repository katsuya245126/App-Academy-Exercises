<h1 class="page-header">Bands</h1>

<ul class='main-list'>

  <% @bands.each do |band| %>
      <%= render band %>
  <% end %>

</ul>

<h4 class="sub-header">Links</h4>

<ul class="page-links">
  <% if current_user.admin? %>
    <li><a href="bands/new" class="button">New band</a></li>
  <% end %>
</ul>